class Converter:
    def __init__(self, voltage,maxVol,bitNum):
        self.voltage=voltage
        self.maxVol=maxVol
        self.bitNum=bitNum

    def toDigital(self):
        newV=bin(round(self.voltage/5*1023))
        return newV


def main():
    c1= Converter(2.7)
    print(c1.toDigital())

main()
